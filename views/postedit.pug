extends base

block content 
    h4 게시글 수정하기
    form(method="POST") 
        input(type="text", name="title",value=`${post.title}`,placeholder="게시글 제목",required,maxlength="100")
        input(type="text", name="location",value=`${post.location}`,placeholder="만날 장소",required,maxlength="100")
        input(type="text", name="meetTime",value=`${post.meetTime}`,placeholder="만날 시간",required,maxlength="100")
        input(type="text", name="requirements",value=`${post.requirements}`,placeholder="요구 사항",required,maxlength="200")
        input(type="submit" , value="저장")